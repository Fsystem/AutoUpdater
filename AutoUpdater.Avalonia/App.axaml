<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="CarinaStudio.AutoUpdater.App">
	
	<!-- Resources -->
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Strings/Default.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
	
	<!-- Styles -->
	<Application.Styles>
		<FluentTheme Mode="Light"/>
	</Application.Styles>
    
</Application>
